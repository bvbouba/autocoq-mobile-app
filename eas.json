{
  "cli": {
    "version": ">= 15.0.9",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "android": {
        "image": "sdk-52",
        "env": {
          "NODE_VERSION": "20"
        },
        "build": {
          "pre-build": [
            "nvm install 20",
            "nvm use 20"
          ]
        }
      },
      "ios": {
        "image": "latest",
        "env": {
          "NODE_VERSION": "20"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}