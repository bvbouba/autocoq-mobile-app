{
  "cli": {
    "version": ">= 15.0.9",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "NODE_VERSION": "20",
        "EXPO_USE_NODE_VERSION": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "NODE_VERSION": "20",
        "EXPO_USE_NODE_VERSION": "true",
        "RCT_NEW_ARCH_ENABLED": "1",
        "EXPO_USE_NEW_ARCHITECTURE": "true"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "env": {
        "NODE_VERSION": "20",
        "EXPO_USE_NODE_VERSION": "true",
         "RCT_NEW_ARCH_ENABLED": "1",
        "EXPO_USE_NEW_ARCHITECTURE": "true"
      },
      "android": {
        "image": "sdk-52"
      },
      "ios": {}
    }
  },
  "submit": {
    "production": {}
  }
}
