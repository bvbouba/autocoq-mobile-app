{
  "cli": {
    "version": ">= 15.0.9",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "android": {
        "image": "sdk-51",
        "env": {
          "NODE_VERSION": "20"
        }
      },
      "ios": {
      "env": {
        "NODE_VERSION": "20"
      }
    }
    }
  },
  "submit": {
    "production": {}
  }
}
