{
  "cli": {
    "version": ">= 15.0.9",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "android": {
        "image": "ubuntu-22.04-jdk-17-ndk-r26b",
        "env": {
          "NODE_VERSION": "20"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
